page{background: #F5F5F5;}
/* header 样式 */
.header{width: 100%; height: 44px; font-size: 0;  background: #FFF; position: fixed; top: 0; left: 0; z-index: 2;}
.header .header-l{height: 100%; line-height: 43px; padding: 0 10px; box-sizing: border-box; font-size: 15px;  display: inline-block; vertical-align: top; position: absolute; bottom: 0; left: 0;}
.header .header-l image{width: 14px; height: 14px; margin: 0 3px 0 0; display: inline-block; vertical-align: middle;}
.header .header-l text{line-height: 44px; color: #646464; display: inline-block; vertical-align: middle;}
.header .header-r{width: -webkit-calc(100vw - 85px); width: calc(100vw - 85px);height: 100%; display: inline-block; vertical-align: top; overflow: hidden; position: absolute; right: 0; bottom: 0;}
.header .header-r .search{width: -webkit-calc(100vw - 85px - 88px); width: calc(100vw - 85px - 88px); height: 26px; border-radius: 26px; background: #EFEFEF; display: inline-block; vertical-align: middle; position: relative; top: 1px;}
.header-r .search-img{width: 16px; height: 16px;  display: inline-block; vertical-align: middle; position: absolute; top: 5px; left: 7px;}
.header-r .search-input{width: -webkit-calc(100% - 54px); width: calc(100% - 54px); height: 100%; color: #646464; font-size: 14px; position: absolute; right: 25px; bottom: 0;}
.header-r .search-clear{width: 18px; height: 18px; display: none; border-radius: 50%; background: rgba(0,0,0,.3); position: absolute; top: 4px; right: 7px;}
.header-r .search-clear::before,.header-r .search-clear::after{content: ''; width: 12px; height: 1px; background: #FFF; position: absolute; top: -webkit-calc((18px - 0.5px) / 2); top: calc((18px - 0.5px) / 2); left: 3px;}
.header-r .search-clear::before{transform: rotate(45deg);}
.header-r .search-clear::after{transform: rotate(-45deg);}

.header-r .release{width: 44px; height: 100%; text-align: center; display: inline-block; vertical-align: middle;}
.header-r .release image{width: 20px; height: 20px; margin: 12px 0 12px 5px;}
.header-r .my{width: 44px; height: 100%; text-align: center; display: inline-block; vertical-align: middle;}
.header-r .my image{width: 20px; height: 20px; margin: 12px 0;}

/* 带返回按钮  头部 */
.cui-header{width: 100%; height: 44px; font-size: 0;  background: #FFF; position: fixed; top: 0; left: 0; z-index: 2;}
.cui-header .cui-header-left{width: 44px; height: 44px; text-align: center; display: inline-block; position: absolute; top: 0; left: 0;}
.cui-header .cui-header-left image{width: 18px; height: 18px; margin: 13px 15px 13px 11px;}
.cui-header .cui-header-title{height: 100%; line-height: 44px; text-align: center; font-size: 17px; color: #333; position: absolute; top: 0; left: 44px; right: 44px;}
.cui-header .cui-header-right{width: 44px; height: 44px; text-align: center; display: inline-block; position: absolute; top: 0; right: 0;}
.cui-header .cui-header-right image{width: 22px; height: 22px; margin: 13px auto;}
.cui-header::after{content: ''; width: 100%; height: 1px; -webkit-transform: scaleY(.3); transform: scaleY(.3); background: rgba(100,100,100,.3); position: absolute; bottom: 0; left: 0; z-index: 2;}

.content{width: 100%; padding: 43px 0 0 0; position: relative;}

/* 导航菜单选项卡 */
.nav{width: 100vw; height: 40px; text-align: center; background: #FFF; position: fixed; top: 44px; left: 0; z-index: 2;}
.nav::after{content: ''; width: 100vw; height: 1px; -webkit-transform: scaleY(.3); transform: scaleY(.3); background: rgba(100,100,100,.4); position: absolute; bottom: 0px; left: 0px;}
.nav .nav-item{padding: 0 30px; height: 100%; line-height: 40px; color: #646464; font-size: 16px; display: inline-block; vertical-align: top;}
.nav .nav-item.active{color: #FF9204; font-size: 17px; position: relative;}
.nav .nav-item.active::after{content: ""; width: -webkit-calc(100% - 50px); width: calc(100% - 50px); height: 2px; background: #FF9204; border-radius: 3px;  position: absolute; bottom: 0px; left: 25px; z-index: 3;}

/* 排序菜单 */
.orderby-items{width: 100%; height: 40px; display: flex; justify-content: space-around; background: #FFF; position: fixed; top: 84px; left: 0; z-index: 2;}
.orderby-items::after{content: ''; width: 100%; height: 1px; -webkit-transform: scaleY(.3); transform: scaleY(.3); background: rgba(100,100,100,.3); position: absolute; bottom: 0px; left: 0; z-index: 1;}
.orderby-item{width: 25%; height: 100%; line-height: 40px; text-align: center; display: inline-block; vertical-align: middle; position: relative;}
.orderby-item:last-child:before{content: ''; width: 1px; height: 20px; -webkit-transform: scaleX(.3); transform: scaleX(.3); background: rgba(100,100,100,.3); position: absolute; top: 10px; left: 0; z-index: 1;}
.orderby-item text{height: 100%; line-height: 40px; font-size: 14px; color: #646464; display: inline-block ;vertical-align: top;}
.orderby-item.active text{color: #FF9204;}
.orderby-item image{width: 15px; height: 8px; display: inline-block; margin: 16px 0 16px 10px; vertical-align: top;}
.orderby-item:last-child image{width: 18px; height: 18px; margin: 11px 0 11px 10px;}
 
/* 无数据展示 */
.noData{width: 100%; text-align: center; margin: 50% 0 0 0; -moz-transform: translate(0, -50%); -ms-transform: translate(0, -50%); -webkit-transform: translate(0, -50%); transform: translate(0, -50%);}
.noData image{width: 50%; margin: 0 auto;}
.noData view{width: 100%; line-height: 20px; font-size: 12px; color: #909090;}

/* lists 数据列表 */
.lists{width: 100%;}
.list{width: 100%; margin-bottom: 7px; background: #FFF; position: relative;}
.list:last-child{margin-bottom: 0px;}
.list::before,.list::after{content: ''; width: 100%; height: 1px; -webkit-transform: scaleY(.3); transform: scaleY(.3); background: rgba(100,100,100,.3); position: absolute; left: 0; z-index: 1;}
.list::before{top: 0px;}
.list::after{bottom: 0px;}
.lists .list:last-child::after{display: none;}
.list-top{width: 100%; padding: 12px; box-sizing: border-box;}
.list-top-l{width: -webkit-calc(100% - 44px); width: calc(100% - 44px); display: inline-block; vertical-align: middle;}
.user-avatar{width: 45px; height: 45px; border-radius: 50%; background: #F5F5F5; display: inline-block; vertical-align: middle;}
.user-name{max-width: -webkit-calc(100% - 45px - 90px); max-width: calc(100% - 45px - 90px); line-height: 30px; text-indent: 10px; font-size: 16px; color: #646464; display: inline-block; vertical-align: middle;}
.user-level{width: auto; display: inline-block; vertical-align: middle;}
.user-level image{width: 17px; height: 17px; margin-left: 10px; position: relative; top: 0px; display: inline-block; vertical-align: middle;}
.list-top-r{width: 44px; height: 45px; text-align: right; display: inline-block; vertical-align: middle;}
.list-top-r .top-r-1{width: 25px; height: 20px; line-height: 18px; margin: 12.5px 0; text-align: center; font-size: 15px; border-radius: 3px; background: #FF9204; color: #FFF; float: right;}
.list-top-r .top-r-2{height: 100%; line-height: 45px; font-size: 13px; color: #909090; text-align: right;}
.list-top-r .top-r-3{width: 25px; height: 20px; margin: 12.5px 0; float: right;}
.list-top-r .top-r-3 image{width: 25px; height: 25px; vertical-align: middle;}
.list-middle{width: 100%; padding: 0 12px; box-sizing: border-box;}
.list-middle .imgs{width: 100%; font-size: 0;}
.list-middle image{width: 100%; max-height: -webkit-calc((100vw - 40px) / 2); max-height: calc((100vw - 40px) / 2); border-radius: 5px; margin-bottom: 10px; background: #F5F5F5;}
.list-middle .list-title{width: 100%; line-height: 25px; display: block; font-size: 17px; color: #333; margin-bottom: 10px;}
.list-middle .list-describe{width: 100%; line-height: 25px; margin-bottom: 10px; font-size: 14px; color: #7B7B7B;}
.list-middle .referees{width: 100%; line-height: 25px; margin: 10px 0; font-size: 13px; color: #909090;}
.list-down{width: 100%; padding: 0 12px 12px 12px; box-sizing: border-box;}
.list-down .address,.list-down .shop-name,.list-down .price,.list-down .rating{line-height: 25px; font-size: 15px; color: #7B7B7B; display: inline-block; margin-right: 10px; vertical-align: middle;}
.list-down .price{font-size: 16px;}
.list-down .rating{float: right; margin-right: 0;}

/* 普通列表图片有两张时 */
.list.twoimgs .list-middle image{width: -webkit-calc((100% - 10px) / 2); width: calc((100% - 10px) / 2); border-radius: 0; display: inline-block;}
.list.twoimgs .list-middle image:first-child{ border-top-left-radius: 5px; border-bottom-left-radius: 5px; margin-right: 10px;}
.list.twoimgs .list-middle image:last-child{border-top-right-radius: 5px; border-bottom-right-radius: 5px;}

/* 好餐馆榜单 */
.bangdan.list .user-avatar{width: 40px; height: 40px; border-radius: 0;}
.bangdan.list .user-name{color: #FF9204;}
.bangdan.list .list-middle image{width: -webkit-calc((100% - 20px) / 3); width: -webkit-calc((100% - 20px) / 3); border-radius: 0; margin-right: 10px; position: relative;}
.bangdan.list .list-middle image::before{content: ''; width: 100%; height: 100%; background: rgba(0,0,0,.3); position: absolute; top: 0; left: 0;}
.bangdan.list .list-middle image::after{content: '牛人榜'; width: 100%; height: 20px; line-height: 20px; font-size: 15px; color: #FFF; text-align: center; position: absolute; top: -webkit-calc((100% - 20px) / 2); top: calc((100% - 20px) / 2); left: 0; z-index: 1;}
.bangdan.list .list-middle image:first-child{ border-top-left-radius: 5px; border-bottom-left-radius: 5px;}
.bangdan.list .list-middle image:nth-child(1)::after{content: '牛人榜';}
.bangdan.list .list-middle image:nth-child(2)::after{content: '牛帖榜';}
.bangdan.list .list-middle image:nth-child(3)::after{content: '好友帖';}
.bangdan.list .list-middle image:last-child{margin-right: 0px; border-top-right-radius: 5px; border-bottom-right-radius: 5px;}
/* 抽奖广告 */
.guanggao.list .user-avatar{width: 40px; height: 40px; border-radius: 0;}
.guanggao.list .user-name{color: #FF9204;}
.guanggao.list .list-middle{padding-bottom: 10px;}
.guanggao.list .list-middle image{width: 100%; border-radius: 5px;}

