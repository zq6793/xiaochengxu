/* 自定义 dialog 模态弹窗样式 */
/* 遮罩层样式 */
.cui-mask{width: 100%; height: 100%; background: rgba(0,0,0,.4); animation: cui-fade-in .2s ease-out forwards; position: fixed; top: 0px; left: 0px; z-index: 3;}
/* dialog 模态弹窗样式 */
.cui-dialog{width: 100vw;height: 100vh;animation: cui-fade-in .2s ease-out forwards;position: fixed;top: 0;left: 0;z-index: 4;}
.cui-dialog-main{width: 80%;background: #fff;border-radius: 5px; animation: cui-scale-in-tosmall .2s ease-out forwards;position: absolute;top: 30%;left: 10%;z-index: 4;}
.cui-dialog-title{width: 100%;height: 40px;line-height: 55px;position: relative;font-size: 17px;display: inline-block;border-top-left-radius: 5px;border-top-right-radius: 5px;text-align: center;color: #333;box-sizing: border-box;}
.cui-dialog-content{width: 100%;max-height: 70vh;line-height: 25px;font-size: 15px;color: #333;text-align: center;display: inline-block;overflow-y: scroll;padding: 30px 20px 25px 20px;box-sizing: border-box;}
.cui-dialog-content::-webkit-scrollbar {width: 1px;}
.cui-dialog-down{width: 100%;height: 50px;text-align: right;position: relative;overflow: hidden;}
.cui-dialog-down-btn{width: 50%;height: 100%;display: inline-block;font-size: 16px;color: var(--cui-color);text-align: center;position: relative;}
.cui-dialog-main-style-1 .cui-dialog-down{height: 50px;}
.cui-dialog-main-style-1 .cui-dialog-down-btn{line-height: 50px;}
.cui-dialog-main-style-1 .cui-dialog-down:before{content: '';width: 100%;height: 1px;-webkit-transform: scaleY(.3);transform: scaleY(.3);background: rgba(100,100,100,.4);position: absolute;top: 0;left: 0;z-index: 1;}
.cui-dialog-main-style-1 .cui-dialog-down-btn:first-child{border-bottom-left-radius: 5px;}
.cui-dialog-main-style-1 .cui-dialog-down-btn:last-child{border-bottom-right-radius: 5px;}
.cui-dialog-main-style-1 .cui-dialog-down-btn:first-child:after{content: '';width: 1px;height: 100%;-webkit-transform: scaleX(.3);transform: scaleX(.3);background: rgba(100,100,100,.4);position: absolute;top: 0;right: 0;z-index: 1;}
.cui-dialog-main-style-1 .cui-dialog-down-btn:last-child:after{display: none;}
.cui-dialog-down-cancel-btn{color: #646464;}
.cui-dialog-down-confirm-btn{color: #FF9204;}

/* 录音弹窗样式 */
.cui-record{width: 100vw;height: 100vh;opacity: 0;animation: cui-fade-in .2s ease-out forwards;position: fixed;top: 0;left: 0;z-index: 4;}
.cui-record .cui-record-main{width: -webkit-calc(100vw - 20px);width: calc(100vw - 20px); max-height: 60vh;overflow: hidden;border-radius: 5px; background: #FFF; bottom: -60px; animation: cui-slide-up .55s cubic-bezier(0.68, -0.55, 0.27, 1.35) forwards; position: absolute;	left: 10px;z-index: 4;}
.cui-record .cui-record-title{	width: 100%;min-height: 50px;	line-height: 25px;	padding: 12px 10px;	text-align: center;	color: #646464; margin: 20px 0 0 0;	box-sizing: border-box;	font-size: 16px;	font-weight: 600;	background: #FFF;position: relative;}
.cui-record .cui-record-close{width: 40px; height: 40px; position: absolute; top: 0px; right: 0px;}
.cui-record .cui-record-close image{width: 20px; height: 20px; margin: 10px;}
.cui-record .cui-record-load{width: 100%; height: 60px; text-align: center; margin: 0px;font-size: 10px;}
.cui-record .cui-record-load .rect-animate{ background-color: #FF9204; height: 100%; width: 5px; border-radius: 20px; margin: 0 3px; display: inline-block; -webkit-animation: stretchdelay 1.2s infinite ease-in-out; animation: stretchdelay 1.2s infinite ease-in-out;}
.cui-record .cui-record-load .rect{ background-color: #FF9204; height: 100%; width: 5px; border-radius: 20px; margin: 0 3px; display: inline-block;-webkit-transform: scaleY(0.1); transform: scaleY(0.1);}
.cui-record .cui-record-load .rect-small-1{-webkit-transform: scaleY(0.1); transform: scaleY(0.1);}
.cui-record .cui-record-load .rect-small-2{-webkit-transform: scaleY(0.15); transform: scaleY(0.15);}
.cui-record .cui-record-load .rect-small-3{-webkit-transform: scaleY(0.2); transform: scaleY(0.2);}
.cui-record .cui-record-load .rect2 { -webkit-animation-delay: -1.1s;animation-delay: -1.1s;}
.cui-record .cui-record-load .rect3 { -webkit-animation-delay: -1.0s; animation-delay: -1.0s;}
.cui-record .cui-record-load .rect4 { -webkit-animation-delay: -0.9s; animation-delay: -0.9s;}
.cui-record .cui-record-load .rect5 { -webkit-animation-delay: -0.8s; animation-delay: -0.8s;}
.cui-record .cui-record-load .rect6 { -webkit-animation-delay: -0.7s; animation-delay: -0.7s;}
.cui-record .cui-record-load .rect7 { -webkit-animation-delay: -0.6s; animation-delay: -0.6s;}
.cui-record .cui-record-load .rect8 { -webkit-animation-delay: -0.5s; animation-delay: -0.5s;}
.cui-record .cui-record-load .rect9 { -webkit-animation-delay: -0.4s; animation-delay: -0.4s;}
.cui-record .cui-record-close-tip{width: 100%; line-height: 50px; font-size: 14px; color: #909090; text-align: center;}
.cui-record .cui-record-stopbtn{width: -webkit-calc(100% - 20px); width: calc(100% - 40px); height: 40px; line-height: 40px; text-align: center; border-radius: 5px; background: #FF9204; margin: 30px 20px 20px 20px;}
.cui-record .cui-record-stopbtn image{width: 15px; height: 15px; margin: 12.5px 5px 12.5px 0; display: inline-block; vertical-align: top;}
.cui-record .cui-record-stopbtn text{height: 100%; line-height: 40px; font-size: 15px; margin: 0 5px; display: inline-block; vertical-align: top; color: #FFF;}


/* APP帖子列表数据右上角 点击弹窗样式 */
.dialog-modal{width: 100%; height: 100vh; position: fixed; top: 0; left: 0; z-index: 9999;}
.dialog-modal-mask{width: 100%; height: 100%; background: rgba(0,0,0,0.4); animation: cui-fade-in .2s ease-out forwards; position: fixed; top: 0px; left: 0px; z-index: 3;}
.dialog-modal-main{width: 75%; position: absolute; top: 48%; left: 12.5%; -webkit-transform: translate(0, -50%); transform: translate(0, -50%); z-index: 9999; animation: cui-scale-in .1s ease-out forwards;}
.dialog-modal-bg{width: 100%; height: 100%;}
.dialog-modal-bg image{width: 100%; height: 100%;}
.dialog-modal-title{width: 100%; height: -webkit-calc(100% * 0.25 - 10px); height: calc(100% * 0.25 - 10px); line-height: 60px; text-align: center; font-size: 17px; color: #333; position: absolute; top: 0; left: 0;}

.dialog-modal-lists{width: 100%; padding: 10px; box-sizing: border-box; position: absolute; top: -webkit-calc(100% - 0.25); top: calc(100% * 0.25);}
.dialog-modal-list{width: 100%; font-size: 0; margin: 0 0 5px 0;}
.dialog-modal-list .point{width: 30px; height: 22px; display: inline-block; vertical-align: top;}
.dialog-modal-list .point image{width: 10px; height: 10px; margin: 6px 10px; border-radius: 50%; display: inline-block;}
.dialog-modal-list text{width: -webkit-calc(100% - 30px); width: calc(100% - 30px); line-height: 22px; display: inline-block; font-size: 15px; color: #7B7B7B;}
.dialog-modal-close{width: 100%; height: 35px; text-align: center; position: absolute; bottom: -50px; left: 0; z-index: 9999;}
.dialog-modal-close image{width: 35px; height: 35px; display: inline-block;}

/* loading-square 样式设置 */
.cui-loading.cui-loading-square{
	width: 100vh;
	height: 100vh;	
	position: absolute;
	top: 0;
	left: 0;
}
.cui-loading.cui-loading-square .cui-loading-main{
	width: 100%;
	height: 100%;
    text-align: center;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 4;
	background: #FFF;
	animation: cui-fade-in .0s ease-out forwards;
}
.cui-loading.cui-loading-square .cui-loading-animate{
	width: 15px;
	height: 14px;	
    display: block;
	position: absolute;
    top: -webkit-calc((100vh - 80px) / 2); 
    top: calc((100vh - 80px) / 2);   
    left: -webkit-calc((100vw - 15px) / 2); 
    left: calc((100vw - 15px) / 2);   
}
.cui-loading.cui-loading-square .cui-loading-animate view{
	width: 14px;
	height: 14px;
	display: inline-block;
	border-radius: 1px;
	background: #2cb0b2;
	top: -webkit-calc((100% - 14px) / 2);
	top: calc((100% - 14px) / 2);
	position: absolute;
}
.cui-loading.cui-loading-square .cui-loading-animate view:first-child{
	background: #ee6362;
	left: -14px;
	animation: cui-loading-square-span1-translate 1.66s ease-in-out infinite,  cui-loading-square-span1-zindex .55s ease-in-out infinite;
}
.cui-loading.cui-loading-square .cui-loading-animate view:last-child{
	background: #FF9204;
	left: 100%;	
	animation: cui-loading-square-span2-translate 1.66s ease-in-out infinite,  cui-loading-square-span2-zindex .55s ease-in-out infinite;
}
.cui-loading.cui-loading-square .cui-loading-msg{
	width: 100vw;
	height: 40px;
	line-height: 45px;
	font-size: 13px;
	color: #909090;
	text-align: center;
    position: absolute;
    top: -webkit-calc((100vh - 45px) / 2); 
    top: calc((100vh - 45px) / 2);   
    left: 0; 
}



/* 动画设计 */
/* fade-in + fade-out */
@-webkit-keyframes cui-fade-in{
	0%{opacity: 0;}
	100%{opacity: 1;}
}
@keyframes cui-fade-in{
	0%{opacity: 0;}
	100%{opacity: 1;}
}
/* cui-scale-in-tosmall + cui-scale-out-tosmall */
@-webkit-keyframes cui-scale-in-tosmall{
	0%{-webkit-transform: scale(1.2); transform: scale(1.2);}
	100%{-webkit-transform: scale(1); transform: scale(1);}
}
@keyframes cui-scale-in-tosmall{
	0%{-webkit-transform: scale(1.2); transform: scale(1.2);}
	100%{-webkit-transform: scale(1); transform: scale(1);}
}

/* cui-scale-in + cui-scale-out */
@-webkit-keyframes cui-scale-in{
	0%{-webkit-transform: scale(0.8) translate(0, -50%); transform: scale(0.8) translate(0, -50%);}
	100%{-webkit-transform: scale(1) translate(0, -50%); transform: scale(1) translate(0, -50%);}
}
@keyframes cui-scale-in{
	0%{-webkit-transform: scale(0.8) translate(0, -50%); transform: scale(0.8) translate(0, -50%);}
	100%{-webkit-transform: scale(1) translate(0, -50%); transform: scale(1) translate(0, -50%);}
}

/* cui-slide-up + cui-slide-down */
@-webkit-keyframes cui-slide-up{
	0%{bottom: -60vh;}
	100%{bottom: 10px;}
}
@keyframes cui-slide-up{
	0%{bottom: -60vh;}
	100%{bottom: 10px;}
}

/* loading加载弹窗 loading-square 动画 */
@keyframes cui-loading-square-span1-translate{
	0%{left: -14px; transform: scale(1.1);}
	50%{left: 100%; transform: scale(1);}
	100%{left: -14px; transform: scale(1.1);}
}
@keyframes cui-loading-square-span1-zindex{
	0%{z-index: 0;}
	50%{z-index: 1;}
	100%{z-index: 0;}
}
@keyframes cui-loading-square-span2-translate{
	0%{left: 100%; transform: scale(1);}
	50%{left: -14px; transform: scale(1.1);}
	100%{left: 100%; transform: scale(1);}
}
@keyframes cui-loading-square-span2-zindex{
	0%{z-index: 1;}
	50%{z-index: 0;}
	100%{z-index: 1;}
}

/* 录音弹窗动画 */
@-webkit-keyframes stretchdelay {
  0%, 40%, 100% { -webkit-transform: scaleY(0.4) } 
  20% { -webkit-transform: scaleY(1.0) }
}
@keyframes stretchdelay {
  0%, 40%, 100% {
    transform: scaleY(0.4);
    -webkit-transform: scaleY(0.4);
  }  20% {
    transform: scaleY(1.0);
    -webkit-transform: scaleY(1.0);
  }
}

/* 排序菜单显示/隐藏动画 */
@-webkit-keyframes menu-slide-in{
	0%{top: -50vh;}
	100%{top: 123px;}
}
@keyframes menu-slide-in{
	0%{top: -50vh;}
	100%{top: 123px;}
}
@-webkit-keyframes menu-slide-out{
	0%{top: 123px;}
	100%{top: -50vh;}
}
@keyframes menu-slide-out{
	0%{top: 123px;}
	100%{top: -50vh;}
}